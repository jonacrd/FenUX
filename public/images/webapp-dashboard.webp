<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="webappGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00e3a2;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#9be15d;stop-opacity:1" />
    </linearGradient>
    <filter id="webappShadow">
      <feDropShadow dx="0" dy="8" stdDeviation="16" flood-color="#00000040"/>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="rgba(255,255,255,0.95)" rx="20" stroke="rgba(0,0,0,0.1)" stroke-width="1" filter="url(#webappShadow)"/>
  
  <!-- Header -->
  <text x="400" y="40" font-family="Arial, sans-serif" font-size="24" font-weight="700" fill="#333" text-anchor="middle">Dashboard de Ventas</text>
  <text x="400" y="65" font-family="Arial, sans-serif" font-size="14" fill="#666" text-anchor="middle">Panel de control completo para tu negocio</text>
  
  <!-- Main dashboard area -->
  <rect x="50" y="100" width="700" height="350" fill="rgba(0,0,0,0.02)" rx="12" stroke="rgba(0,0,0,0.05)" stroke-width="1"/>
  
  <!-- Sidebar -->
  <rect x="70" y="120" width="120" height="310" fill="rgba(0,0,0,0.02)" rx="8"/>
  <rect x="80" y="140" width="100" height="30" fill="url(#webappGrad)" rx="4"/>
  <text x="130" y="160" font-family="Arial, sans-serif" font-size="11" font-weight="600" fill="white" text-anchor="middle">Dashboard</text>
  
  <rect x="80" y="180" width="100" height="25" fill="rgba(0,0,0,0.05)" rx="3"/>
  <text x="130" y="197" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">Ventas</text>
  
  <rect x="80" y="215" width="100" height="25" fill="rgba(0,0,0,0.05)" rx="3"/>
  <text x="130" y="232" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">Productos</text>
  
  <rect x="80" y="250" width="100" height="25" fill="rgba(0,0,0,0.05)" rx="3"/>
  <text x="130" y="267" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">Clientes</text>
  
  <rect x="80" y="285" width="100" height="25" fill="rgba(0,0,0,0.05)" rx="3"/>
  <text x="130" y="302" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">Reportes</text>
  
  <rect x="80" y="320" width="100" height="25" fill="rgba(0,0,0,0.05)" rx="3"/>
  <text x="130" y="337" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">Configuración</text>
  
  <!-- Main content area -->
  <rect x="210" y="120" width="520" height="310" fill="rgba(0,0,0,0.01)" rx="8"/>
  
  <!-- Stats cards -->
  <rect x="230" y="140" width="150" height="80" fill="rgba(255,255,255,0.8)" rx="8" stroke="rgba(0,0,0,0.05)" stroke-width="1"/>
  <text x="305" y="160" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Ventas del Mes</text>
  <text x="305" y="185" font-family="Arial, sans-serif" font-size="24" font-weight="800" fill="url(#webappGrad)" text-anchor="middle">$12,430</text>
  <text x="305" y="205" font-family="Arial, sans-serif" font-size="10" fill="#28ca42" text-anchor="middle">↑ +18% vs mes anterior</text>
  
  <rect x="400" y="140" width="150" height="80" fill="rgba(255,255,255,0.8)" rx="8" stroke="rgba(0,0,0,0.05)" stroke-width="1"/>
  <text x="475" y="160" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Conversión</text>
  <text x="475" y="185" font-family="Arial, sans-serif" font-size="24" font-weight="800" fill="url(#webappGrad)" text-anchor="middle">18%</text>
  <text x="475" y="205" font-family="Arial, sans-serif" font-size="10" fill="#28ca42" text-anchor="middle">↑ +3% vs mes anterior</text>
  
  <rect x="570" y="140" width="150" height="80" fill="rgba(255,255,255,0.8)" rx="8" stroke="rgba(0,0,0,0.05)" stroke-width="1"/>
  <text x="645" y="160" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Usuarios</text>
  <text x="645" y="185" font-family="Arial, sans-serif" font-size="24" font-weight="800" fill="url(#webappGrad)" text-anchor="middle">1,247</text>
  <text x="645" y="205" font-family="Arial, sans-serif" font-size="10" fill="#28ca42" text-anchor="middle">↑ +8% vs mes anterior</text>
  
  <!-- Chart area -->
  <rect x="230" y="240" width="490" height="120" fill="rgba(255,255,255,0.8)" rx="8" stroke="rgba(0,0,0,0.05)" stroke-width="1"/>
  <text x="475" y="260" font-family="Arial, sans-serif" font-size="14" font-weight="600" fill="#333" text-anchor="middle">Tendencia de Ventas (Últimos 6 meses)</text>
  
  <!-- Simple chart bars -->
  <rect x="250" y="320" width="20" height="40" fill="url(#webappGrad)" rx="2"/>
  <rect x="280" y="310" width="20" height="50" fill="url(#webappGrad)" rx="2"/>
  <rect x="310" y="300" width="20" height="60" fill="url(#webappGrad)" rx="2"/>
  <rect x="340" y="315" width="20" height="45" fill="url(#webappGrad)" rx="2"/>
  <rect x="370" y="305" width="20" height="55" fill="url(#webappGrad)" rx="2"/>
  <rect x="400" y="320" width="20" height="40" fill="url(#webappGrad)" rx="2"/>
  <rect x="430" y="310" width="20" height="50" fill="url(#webappGrad)" rx="2"/>
  <rect x="460" y="300" width="20" height="60" fill="url(#webappGrad)" rx="2"/>
  <rect x="490" y="315" width="20" height="45" fill="url(#webappGrad)" rx="2"/>
  <rect x="520" y="305" width="20" height="55" fill="url(#webappGrad)" rx="2"/>
  <rect x="550" y="320" width="20" height="40" fill="url(#webappGrad)" rx="2"/>
  <rect x="580" y="310" width="20" height="50" fill="url(#webappGrad)" rx="2"/>
  <rect x="610" y="300" width="20" height="60" fill="url(#webappGrad)" rx="2"/>
  <rect x="640" y="315" width="20" height="45" fill="url(#webappGrad)" rx="2"/>
  <rect x="670" y="305" width="20" height="55" fill="url(#webappGrad)" rx="2"/>
  <rect x="700" y="320" width="20" height="40" fill="url(#webappGrad)" rx="2"/>
  
  <!-- Chart labels -->
  <text x="260" y="350" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">Ene</text>
  <text x="350" y="350" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">Mar</text>
  <text x="450" y="350" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">May</text>
  <text x="550" y="350" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">Jul</text>
  <text x="650" y="350" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">Sep</text>
  
  <!-- Action buttons -->
  <rect x="230" y="380" width="100" height="35" fill="url(#webappGrad)" rx="17"/>
  <text x="280" y="402" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="white" text-anchor="middle">Ver Reporte</text>
  
  <rect x="350" y="380" width="100" height="35" fill="rgba(0,0,0,0.05)" rx="17"/>
  <text x="400" y="402" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Exportar</text>
  
  <rect x="470" y="380" width="100" height="35" fill="rgba(0,0,0,0.05)" rx="17"/>
  <text x="520" y="402" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Configurar</text>
  
  <!-- Success indicator -->
  <rect x="650" y="120" width="40" height="20" fill="#28ca42" rx="10"/>
  <text x="670" y="133" font-family="Arial, sans-serif" font-size="10" font-weight="bold" fill="white" text-anchor="middle">LIVE</text>
</svg>
