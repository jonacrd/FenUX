<svg width="600" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="processGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00E5FF;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#7C4DFF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E91E63;stop-opacity:1" />
    </linearGradient>
    <filter id="processShadow">
      <feDropShadow dx="0" dy="6" stdDeviation="12" flood-color="#00000030"/>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="rgba(255,255,255,0.95)" rx="16" stroke="rgba(0,0,0,0.1)" stroke-width="1" filter="url(#processShadow)"/>
  
  <!-- Header -->
  <text x="300" y="30" font-family="Arial, sans-serif" font-size="18" font-weight="700" fill="#333" text-anchor="middle">Proceso de Trabajo</text>
  
  <!-- Process steps -->
  <!-- Step 1 -->
  <circle cx="80" cy="80" r="25" fill="url(#processGrad)"/>
  <text x="80" y="87" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">1</text>
  <text x="80" y="120" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Brief</text>
  <text x="80" y="135" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">15 min</text>
  
  <!-- Arrow 1 -->
  <path d="M 120 80 L 160 80" stroke="url(#processGrad)" stroke-width="3" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- Step 2 -->
  <circle cx="200" cy="80" r="25" fill="url(#processGrad)"/>
  <text x="200" y="87" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">2</text>
  <text x="200" y="120" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Wireframe</text>
  <text x="200" y="135" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">1 día</text>
  
  <!-- Arrow 2 -->
  <path d="M 240 80 L 280 80" stroke="url(#processGrad)" stroke-width="3" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- Step 3 -->
  <circle cx="320" cy="80" r="25" fill="url(#processGrad)"/>
  <text x="320" y="87" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">3</text>
  <text x="320" y="120" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Desarrollo</text>
  <text x="320" y="135" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">3-4 días</text>
  
  <!-- Arrow 3 -->
  <path d="M 360 80 L 400 80" stroke="url(#processGrad)" stroke-width="3" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- Step 4 -->
  <circle cx="440" cy="80" r="25" fill="url(#processGrad)"/>
  <text x="440" y="87" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">4</text>
  <text x="440" y="120" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">QA</text>
  <text x="440" y="135" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">1 día</text>
  
  <!-- Arrow 4 -->
  <path d="M 480 80 L 520 80" stroke="url(#processGrad)" stroke-width="3" fill="none" marker-end="url(#arrowhead)"/>
  
  <!-- Step 5 -->
  <circle cx="560" cy="80" r="25" fill="url(#processGrad)"/>
  <text x="560" y="87" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">5</text>
  <text x="560" y="120" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Entrega</text>
  <text x="560" y="135" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">1 día</text>
  
  <!-- Details section -->
  <rect x="40" y="180" width="520" height="100" fill="rgba(0,0,0,0.02)" rx="8"/>
  <text x="300" y="205" font-family="Arial, sans-serif" font-size="14" font-weight="600" fill="#333" text-anchor="middle">Cada paso incluye</text>
  
  <text x="80" y="230" font-family="Arial, sans-serif" font-size="11" fill="#666">• Comunicación constante</text>
  <text x="80" y="245" font-family="Arial, sans-serif" font-size="11" fill="#666">• Revisiones en tiempo real</text>
  <text x="80" y="260" font-family="Arial, sans-serif" font-size="11" fill="#666">• Feedback inmediato</text>
  
  <text x="300" y="230" font-family="Arial, sans-serif" font-size="11" fill="#666">• Diseño mobile-first</text>
  <text x="300" y="245" font-family="Arial, sans-serif" font-size="11" fill="#666">• Optimización SEO</text>
  <text x="300" y="260" font-family="Arial, sans-serif" font-size="11" fill="#666">• Performance 90+</text>
  
  <text x="520" y="230" font-family="Arial, sans-serif" font-size="11" fill="#666">• Testing completo</text>
  <text x="520" y="245" font-family="Arial, sans-serif" font-size="11" fill="#666">• Deploy automático</text>
  <text x="520" y="260" font-family="Arial, sans-serif" font-size="11" fill="#666">• Soporte 7 días</text>
  
  <!-- Arrow markers -->
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="url(#processGrad)"/>
    </marker>
  </defs>
</svg>
