<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="workflowGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00E5FF;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#7C4DFF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E91E63;stop-opacity:1" />
    </linearGradient>
    <filter id="workflowShadow">
      <feDropShadow dx="0" dy="8" stdDeviation="16" flood-color="#00000030"/>
    </filter>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="rgba(255,255,255,0.95)" rx="20" stroke="rgba(0,0,0,0.1)" stroke-width="1" filter="url(#workflowShadow)"/>
  
  <!-- Header -->
  <text x="400" y="40" font-family="Arial, sans-serif" font-size="24" font-weight="700" fill="#333" text-anchor="middle">Proceso de Trabajo</text>
  <text x="400" y="65" font-family="Arial, sans-serif" font-size="14" fill="#666" text-anchor="middle">De la idea al resultado en 7 días</text>
  
  <!-- Timeline -->
  <line x1="100" y1="120" x2="700" y2="120" stroke="url(#workflowGrad)" stroke-width="4"/>
  
  <!-- Step 1 -->
  <circle cx="100" cy="120" r="20" fill="url(#workflowGrad)"/>
  <text x="100" y="127" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">1</text>
  <rect x="60" y="160" width="80" height="60" fill="rgba(0,0,0,0.02)" rx="8"/>
  <text x="100" y="180" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Brief</text>
  <text x="100" y="195" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">15 min</text>
  <text x="100" y="210" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">Definir objetivo</text>
  
  <!-- Step 2 -->
  <circle cx="200" cy="120" r="20" fill="url(#workflowGrad)"/>
  <text x="200" y="127" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">2</text>
  <rect x="160" y="160" width="80" height="60" fill="rgba(0,0,0,0.02)" rx="8"/>
  <text x="200" y="180" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Wireframe</text>
  <text x="200" y="195" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">1 día</text>
  <text x="200" y="210" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">Estructura visual</text>
  
  <!-- Step 3 -->
  <circle cx="300" cy="120" r="20" fill="url(#workflowGrad)"/>
  <text x="300" y="127" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">3</text>
  <rect x="260" y="160" width="80" height="60" fill="rgba(0,0,0,0.02)" rx="8"/>
  <text x="300" y="180" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Desarrollo</text>
  <text x="300" y="195" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">3-4 días</text>
  <text x="300" y="210" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">Código + diseño</text>
  
  <!-- Step 4 -->
  <circle cx="400" cy="120" r="20" fill="url(#workflowGrad)"/>
  <text x="400" y="127" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">4</text>
  <rect x="360" y="160" width="80" height="60" fill="rgba(0,0,0,0.02)" rx="8"/>
  <text x="400" y="180" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">QA</text>
  <text x="400" y="195" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">1 día</text>
  <text x="400" y="210" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">Testing completo</text>
  
  <!-- Step 5 -->
  <circle cx="500" cy="120" r="20" fill="url(#workflowGrad)"/>
  <text x="500" y="127" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">5</text>
  <rect x="460" y="160" width="80" height="60" fill="rgba(0,0,0,0.02)" rx="8"/>
  <text x="500" y="180" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Deploy</text>
  <text x="500" y="195" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">1 día</text>
  <text x="500" y="210" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">Vercel + dominio</text>
  
  <!-- Step 6 -->
  <circle cx="600" cy="120" r="20" fill="url(#workflowGrad)"/>
  <text x="600" y="127" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="white" text-anchor="middle">6</text>
  <rect x="560" y="160" width="80" height="60" fill="rgba(0,0,0,0.02)" rx="8"/>
  <text x="600" y="180" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">Entrega</text>
  <text x="600" y="195" font-family="Arial, sans-serif" font-size="10" fill="#666" text-anchor="middle">1 día</text>
  <text x="600" y="210" font-family="Arial, sans-serif" font-size="9" fill="#666" text-anchor="middle">Video + soporte</text>
  
  <!-- Details section -->
  <rect x="50" y="260" width="700" height="200" fill="rgba(0,0,0,0.02)" rx="12"/>
  <text x="400" y="285" font-family="Arial, sans-serif" font-size="16" font-weight="600" fill="#333" text-anchor="middle">¿Qué incluye cada paso?</text>
  
  <!-- Left column -->
  <text x="80" y="315" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333">Comunicación</text>
  <text x="80" y="335" font-family="Arial, sans-serif" font-size="10" fill="#666">• Updates diarios</text>
  <text x="80" y="350" font-family="Arial, sans-serif" font-size="10" fill="#666">• Feedback inmediato</text>
  <text x="80" y="365" font-family="Arial, sans-serif" font-size="10" fill="#666">• Revisiones en tiempo real</text>
  <text x="80" y="380" font-family="Arial, sans-serif" font-size="10" fill="#666">• Soporte 24/7</text>
  
  <!-- Center column -->
  <text x="300" y="315" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333">Técnico</text>
  <text x="300" y="335" font-family="Arial, sans-serif" font-size="10" fill="#666">• Mobile-first design</text>
  <text x="300" y="350" font-family="Arial, sans-serif" font-size="10" fill="#666">• SEO optimizado</text>
  <text x="300" y="365" font-family="Arial, sans-serif" font-size="10" fill="#666">• Performance 90+</text>
  <text x="300" y="380" font-family="Arial, sans-serif" font-size="10" fill="#666">• Analytics integrado</text>
  
  <!-- Right column -->
  <text x="520" y="315" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333">Entrega</text>
  <text x="520" y="335" font-family="Arial, sans-serif" font-size="10" fill="#666">• Código fuente</text>
  <text x="520" y="350" font-family="Arial, sans-serif" font-size="10" fill="#666">• Deploy automático</text>
  <text x="520" y="365" font-family="Arial, sans-serif" font-size="10" fill="#666">• Video explicativo</text>
  <text x="520" y="380" font-family="Arial, sans-serif" font-size="10" fill="#666">• Soporte 7 días</text>
  
  <!-- Bottom note -->
  <text x="400" y="440" font-family="Arial, sans-serif" font-size="11" fill="#666" text-anchor="middle">Proceso probado • Resultados garantizados • Sin costos ocultos</text>
</svg>
