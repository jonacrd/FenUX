<svg width="800" height="500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="landingGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#00E5FF;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#7C4DFF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E91E63;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge> 
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
    <filter id="shadow">
      <feDropShadow dx="0" dy="8" stdDeviation="16" flood-color="#00000040"/>
    </filter>
  </defs>
  
  <!-- Background with mesh effect -->
  <rect width="100%" height="100%" fill="url(#landingGrad)" rx="24"/>
  
  <!-- Main device mockup -->
  <rect x="100" y="80" width="600" height="340" fill="rgba(255,255,255,0.95)" rx="20" stroke="rgba(255,255,255,0.3)" stroke-width="2" filter="url(#shadow)"/>
  
  <!-- Browser header -->
  <rect x="100" y="80" width="600" height="50" fill="rgba(240,240,240,0.9)" rx="20" rx="20" rx="0" rx="0"/>
  <circle cx="130" cy="105" r="6" fill="#ff5f57"/>
  <circle cx="150" cy="105" r="6" fill="#ffbd2e"/>
  <circle cx="170" cy="105" r="6" fill="#28ca42"/>
  
  <!-- Navigation -->
  <rect x="200" y="95" width="400" height="20" fill="rgba(0,0,0,0.1)" rx="10"/>
  <text x="400" y="108" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333" text-anchor="middle">www.ejemplo.com</text>
  
  <!-- Hero content -->
  <rect x="120" y="150" width="560" height="80" fill="rgba(0,0,0,0.05)" rx="12"/>
  <text x="400" y="180" font-family="Arial, sans-serif" font-size="28" font-weight="800" fill="#333" text-anchor="middle">Landing Page Profesional</text>
  <text x="400" y="210" font-family="Arial, sans-serif" font-size="16" fill="#666" text-anchor="middle">Diseño moderno que convierte visitantes en clientes</text>
  
  <!-- CTA Button -->
  <rect x="300" y="250" width="200" height="50" fill="url(#landingGrad)" rx="25" filter="url(#glow)"/>
  <text x="400" y="280" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white" text-anchor="middle">Comenzar Ahora</text>
  
  <!-- Features grid -->
  <rect x="120" y="320" width="180" height="80" fill="rgba(0,0,0,0.05)" rx="8"/>
  <circle cx="150" cy="340" r="8" fill="url(#landingGrad)"/>
  <text x="170" y="345" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333">Mobile First</text>
  <text x="130" y="365" font-family="Arial, sans-serif" font-size="10" fill="#666">Responsive design</text>
  
  <rect x="320" y="320" width="180" height="80" fill="rgba(0,0,0,0.05)" rx="8"/>
  <circle cx="350" cy="340" r="8" fill="url(#landingGrad)"/>
  <text x="370" y="345" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333">SEO Optimizado</text>
  <text x="330" y="365" font-family="Arial, sans-serif" font-size="10" fill="#666">Mejor ranking</text>
  
  <rect x="520" y="320" width="180" height="80" fill="rgba(0,0,0,0.05)" rx="8"/>
  <circle cx="550" cy="340" r="8" fill="url(#landingGrad)"/>
  <text x="570" y="345" font-family="Arial, sans-serif" font-size="12" font-weight="600" fill="#333">Rápido</text>
  <text x="530" y="365" font-family="Arial, sans-serif" font-size="10" fill="#666">90+ Lighthouse</text>
  
  <!-- Floating elements -->
  <circle cx="50" cy="150" r="20" fill="rgba(255,255,255,0.2)"/>
  <circle cx="750" cy="200" r="15" fill="rgba(255,255,255,0.3)"/>
  <circle cx="80" cy="350" r="12" fill="rgba(255,255,255,0.4)"/>
  <circle cx="720" cy="400" r="18" fill="rgba(255,255,255,0.2)"/>
</svg>